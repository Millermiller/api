{
    "name": "laravel/laravel",
    "version": "0.5.2",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php-laravel": "^3.5",
        "beberlei/doctrineextensions": "^1.2",
        "codeguy/upload": "1.3.2",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "gedmo/doctrine-extensions": "^2.4",
        "guzzlehttp/guzzle": "6.3.3",
        "intervention/image": "^2.4",
        "itsgoingd/clockwork": "4.1.3",
        "laravel-doctrine/extensions": "^1.0",
        "laravel-doctrine/migrations": "^1.1",
        "laravel-doctrine/orm": "1.4.*",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.7.0",
        "laravolt/avatar": "^2.1",
        "predis/predis": "^1.1",
        "profideo-ci/phpexcel": "1.8.0",
        "rap2hpoutre/laravel-log-viewer": "^0.22.1",
        "spatie/laravel-activitylog": "^2.8",
        "sunra/php-simple-html-dom-parser": "1.5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "2.5.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "^1.1",
        "laravel/browser-kit-testing": "^5.1",
        "laravel/homestead": "^8.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.0",
        "reliese/laravel": "^0.0.13",
        "symfony/thanks": "^1.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
          "database/factoriesback"
        ],
        "psr-4": {
            "App\\": "app/",
            "Scandinaver\\": "src/Scandinaver"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate",
          "php artisan ide-helper:generate",
          "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 600,
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
